{
  "version": 2,
  "builds": [
    { "src": "api/index.py", "use": "@vercel/python" },
    { "src": "api/hello.py", "use": "@vercel/python" }
  ],
  "routes": [
    { "src": "/api/hello", "dest": "api/hello.py" },
    { "src": "/api/(.*)", "dest": "api/index.py" },
    { "src": "/", "dest": "frontend/index.html" },
    { "src": "/(.*)", "dest": "frontend/$1" }
  ]
}